{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ourblocktheme/pagebanner",
	"title": "Fictional University Page Banner",
	"description": "A banner between header and page content",
	"category": "design",
	"icon": "format-image",
	"supports": {
		"align": [ "full" ],
		"html": true
	},
	"attributes": {
		"title": {
			"type": "string",
      "default": "Fictional University"
		},
		"subtitle": {
			"type": "string",
      "default": "Welcome to our university"
		},
		"photo": {
			"type": "string",
      "default": "/images/ocean.jpg"
		}
	},
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
